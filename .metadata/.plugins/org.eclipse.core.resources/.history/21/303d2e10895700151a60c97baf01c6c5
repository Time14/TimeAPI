package debug;

public class Debug {
	
	 /* Debug - A debug class that holds helper functions
	  * 		DrawSquare(x, y, w, h)
	  * 		Log(Message)
	  */
	
	public static void drawSquare(int x, int y, int w, int h) {
		
	}
	
	public static void Log(String message) {
		StackTraceElement[] ste = Thread.currentThread().getStackTrace();
		System.out.println("[" + ste[0].getFileName() + " @ " + ste[0].getLineNumber() "] " + message);
	}
	
}
